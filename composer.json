{
    "name": "g4t/documentation",
    "description": "Auto Generate Laravel api Documentation",
    "license": "MIT",
    "authors": [
        {
            "name": "HusseinAlaa",
            "email": "hussein4alaa@gmail.com",
            "homepage": "https://www.linkedin.com/in/hussein4alaa/"
        }
    ],
    "homepage": "https://github.com/hussein4alaa/Laravel-Dark-Documentation-Generator",
    "keywords": ["Laravel", "api Documentation", "Documentation"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "g4t\\Documentation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "g4t\\Documentation\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "g4t\\Documentation\\DocumentationServiceProvider"
            ],
            "aliases": {
                "Documentation": "g4t\\Documentation\\Facades\\Documentation"
            }
        }
    }
}
